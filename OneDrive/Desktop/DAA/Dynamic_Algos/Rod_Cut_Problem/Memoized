def mem_rod_cut_aux(p, n, mem):
    if mem[n] > 0:
        return mem[n]
    if n == 0:
        q = 0
    else:
        q = -float('inf')
        
        for i in range(1,  n + 1):
            q = max(q, p[i] + mem_rod_cut_aux(p, n - i, mem))
        
        mem[n] = q
    
    return q

def mem_rod_cut(p, n):
    mem = {i: -float('inf') for i in range(n + 1)}
    return mem_rod_cut_aux(p, n, mem)